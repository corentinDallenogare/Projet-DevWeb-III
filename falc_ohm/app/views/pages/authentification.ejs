<body class="row align-items-end" style="margin-top: 10%;">

<div id="content" class="m-auto ">
    <h1>Authentification</h1>
    <form id="formAuthentification">
        <fieldset>
            <legend>Connexion</legend>
                <div class="form-group">
                    <label for="adressemail">Entrez votre adresse mail</label>
                    <input type="text" class="form-control" id="adressemail" placeholder="Adresse mail" required>
                </div>

                <div class="form-group">
                    <label for="motdepasse">Entrez votre mot de passe</label>
                    <input type="text" class="form-control form-control-lg" id="motdepasse" placeholder="Mot de passe" required>
                </div>
                <div id="buttonSubmit"></div>
        </fieldset>
    </form>
</div>

